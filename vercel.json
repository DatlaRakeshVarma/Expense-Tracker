{
  "version": 2,
  "builds": [
    {
      "src": "frontend/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/index.js",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ]
}
